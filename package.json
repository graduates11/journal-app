{
  "name": "electron-test-cra",
  "version": "0.1.0",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "draft-js": "^0.11.0",
    "draft-js-plugins-editor": "^2.1.1",
    "electron-is-dev": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.14.2",
    "faker": "^4.1.0",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "shortid": "^2.2.14"
  },
  "scripts": {
    "cra-start": "react-scripts start",
    "cra-build": "react-scripts build",
    "cra-test": "react-scripts test",
    "cra-eject": "react-scripts eject",
    "upgrade-dep": "yarn upgrade-interactive --latest",
    "lint": "eslint --fix .",
    "precommit": "npm run lint",
    "dev": "concurrently \"BROWSER=none npm run cra-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "electron-builder",
    "build": "rm -rf ./dist && yarn run cra-build && yarn run electron-build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.6",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "lodash": "^4.17.14",
    "lodash.template": "^4.5.0",
    "prettier": "^1.18.2",
    "typescript": "^3.4.1",
    "wait-on": "^3.2.0"
  },
  "build": {
    "productName": "electra",
    "appId": "com.dci.electra",
    "files": [
      "build/**/*"
    ]
  }
}
